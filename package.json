{
  "name": "mcaz-desktop",
  "version": "1.1.0",
  "description": "MCAZ Desktop application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder build --publish never",
    "pack-win": "electron-builder build --publish never -w",
    "build-dev": "webpack --watch",
    "build": "webpack"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "MCAZ",
    "Desktop",
    "application",
    "ADR",
    "SAE",
    "AEFI"
  ],
  "author": "IntelliSOFT Kenya Limited",
  "license": "CC0-1.0",
  "devDependencies": {
    "@babel/core": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-loader": "^8.1.0",
    "babel-preset-react": "^6.24.1",
    "dotenv": "^8.2.0",
    "electron": "^10.1.3",
    "electron-builder": "^22.9.1",
    "electron-notarize": "^1.0.0",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0"
  },
  "dependencies": {
    "b64-to-blob": "^1.2.19",
    "bootstrap": "3.4.1",
    "css-loader": "^5.0.0",
    "electron-save-file": "^1.0.2",
    "electron-store": "^6.0.1",
    "file-saver": "^2.0.2",
    "fs-jetpack": "^3.2.0",
    "react": "^16.14.0",
    "react-autosuggest": "^10.0.3",
    "react-bootstrap": "0.31.5",
    "react-datepicker": "^3.3.0",
    "react-dom": "^16.14.0",
    "react-notification-system": "^0.4.0",
    "react-redux": "^7.2.1",
    "react-toastify": "^6.0.9",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-persist-electron-storage": "^2.1.0",
    "redux-thunk": "^2.3.0",
    "x2js": "^3.4.0"
  },
  "build": {
    "appId": "com.intellisoft.mcaz",
    "mac": {
      "category": "com.intellisoft.mcaz",
      "icon": "./assets/icons/mac/icon.icns",
      "gatekeeperAssess": false,
      "hardenedRuntime": true,
      "entitlements": "dist/entitlements.mac.plist",
      "entitlementsInherit": "dist/entitlements.mac.plist"
    },
    "win": {
      "icon": "./assets/icons/win/icon.ico",
      "artifactName": "MCAZ Desktop App",
      "target": [
        "nsis"
      ]
    },
    "afterSign": "scripts/notarize.js"
  }
}
